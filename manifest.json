{
  "name": "Linxtractor",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Extract links from video description.",
	"content_scripts": [
    {
			"run_at": "document_idle",
      "js": [
        "content-script.js"
      ],
      "matches": [
				"https://www.bilibili.com/*"
      ]
    }
  ]
}